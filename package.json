{
  "name": "my-app",
  "version": "1.0.0",
  "main": "./index",
  "scripts": {
    "ios": "npx expo run:ios",
    "ios:device": "npx expo run:ios --device",
    "start:livekit": "python -m server.livekit_worker.main dev",
    "start:api": "uvicorn server.api.main:app --reload --port 8000 --env-file .env",
    "start:ngrok": "bash -c 'source .env && ngrok http --domain=$EXPO_PUBLIC_API_URL 8000'",
    "start:server": "source .venv/bin/activate && concurrently \"npm run start:api\" \"npm run start:ngrok\"",
    "supabase:generate": "supabase gen types typescript --project-id fgaselroozoxiyumasqg > lib/types/supabase.ts",
    "prisma:generate": "prisma generate",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:create": "npx prisma migrate dev --create-only",
    "android": "expo run:android",
    "web": "expo start --web",
    "build:container": "docker build -t glow-fast-api .",
    "run:livekit:container": "docker run -p 8000:8000 --env-file ./.env 'glow-fast-api' python -m server.livekit_worker.main dev",
    "run:api:container": "docker run -p 8000:8000 --env-file ./.env 'glow-fast-api' python main.py start",
    "deploy:secrets:staging": "cat .env.staging | fly secrets import --config ./ops/fly/fly.staging.toml",
    "deploy:secrets:prod": "cat .env.prod | fly secrets import --config ./ops/fly/fly.prod.toml",
    "build:ios": "eas build --platform ios",
    "submit:ios": "eas submit -p ios"
  },
  "dependencies": {
    "@config-plugins/react-native-webrtc": "^9.0.0",
    "@gorhom/bottom-sheet": "^5.0.2",
    "@livekit/react-native": "^2.4.3",
    "@livekit/react-native-expo-plugin": "^1.0.0",
    "@livekit/react-native-webrtc": "^125.0.5",
    "@prisma/client": "5.17.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/drawer": "^6.7.2",
    "@rneui/themed": "^4.0.0-rc.8",
    "@segment/analytics-react-native": "2.19.4",
    "@segment/sovran-react-native": "1.1.1",
    "@sentry/react-native": "~5.24.3",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.7.7",
    "axios-hooks": "^5.0.2",
    "concurrently": "^8.2.2",
    "expo": "~51.0.39",
    "expo-apple-authentication": "~6.4.1",
    "expo-audio": "^0.2.3",
    "expo-av": "~14.0.7",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.24",
    "expo-status-bar": "~1.12.1",
    "livekit-client": "^2.5.9",
    "openai": "^4.69.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-fast-encoder": "^0.1.12",
    "react-native-fetch-api": "^3.0.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.2",
    "web-streams-polyfill": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^22.7.9",
    "@types/react": "~18.2.45",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "mem0ai": "^1.0.24",
    "prisma": "5.17.0",
    "tiktoken": "^1.0.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "vite-node": "^2.1.4",
    "vitest": "^2.1.4"
  },
  "private": true
}
