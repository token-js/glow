{
  "name": "my-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:agent": "python ./python/agent/index.py dev",
    "start:auth": "uvicorn python.auth.index:app --reload --port 8000 --env-file .env",
    "start:ngrok": "bash -c 'source .env && ngrok http --domain=$NGROK_URL 8000'",
    "start:server": "source .venv/bin/activate && pip install -r python/requirements.txt && concurrently \"npm run start:agent\" \"npm run start:auth\" \"npm run start:ngrok\"",
    "prisma:generate": "prisma generate",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma deploy",
    "prisma:create": "npx prisma migrate dev --create-only",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@config-plugins/react-native-webrtc": "^9.0.0",
    "@gorhom/bottom-sheet": "^5.0.2",
    "@livekit/react-native": "^2.4.3",
    "@livekit/react-native-expo-plugin": "^1.0.0",
    "@livekit/react-native-webrtc": "^125.0.5",
    "@prisma/client": "5.17.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/drawer": "^6.7.2",
    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/supabase-js": "^2.45.4",
    "@tamagui/babel-plugin": "^1.116.7",
    "@tamagui/config": "^1.116.7",
    "axios": "^1.7.7",
    "axios-hooks": "^5.0.2",
    "concurrently": "^8.2.2",
    "expo": "~51.0.28",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.28",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "livekit-client": "^2.5.9",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-url-polyfill": "^2.0.0",
    "tamagui": "^1.116.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^22.7.9",
    "@types/react": "~18.2.45",
    "prisma": "5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "private": true
}
